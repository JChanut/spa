<section id="clusters-view" class="view">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Pdf</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: clusters">
            <tr>
                <td data-bind="text: Name"></td>
                <td>
                    <a target="_blank" data-bind="text: PdfUrl, attr: { href: PdfUrl }"></a>
                </td>
                <td>
                    <input type="file" data-url="/api/file" data-bind="fileUpload: File, uniqueName: true">
                </td>
                <td>
                    <a href="#" class="btn" data-bind="click: $parent.updateCluster">Save</a>
                </td>
            </tr>
        </tbody>
    </table>
</section>
